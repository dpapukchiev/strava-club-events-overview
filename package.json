{
  "name": "strava-scraper",
  "version": "1.0.0",
  "description": "App to poll Strava for club rides in Berlin",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "serve": "ts-node src/server.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "p-queue": "^7.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.22",
    "@types/node": "^20.8.10",
    "@types/p-queue": "^3.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
